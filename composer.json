{
    "name": "crazymeeks/phpcacher",
    "description": "A PHP caching library",
    "license": "MIT",
    "keywords": ["PHP", "PHP Cache", "PHP Cache package"],
    "authors": [
        {
            "name": "crazymeeks",
            "email": "jeffclaud17@gmail.com"
        }
    ],
    "autoload":{
